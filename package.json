{
	"name": "@cc-workflows/citation-manager",
	"version": "1.0.0",
	"description": "Citation link validator and manager for Obsidian markdown files",
	"type": "module",
	"private": true,
	"main": "dist/citation-manager.js",
	"bin": {
		"citation-manager": "./dist/citation-manager.js"
	},
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run build",
		"test": "vitest run",
		"test:watch": "vitest",
		"start": "node dist/citation-manager.js",
		"validate:ts-conversion": "node --loader ts-node/esm scripts/validate-typescript-conversion.ts"
	},
	"keywords": [
		"citation",
		"validator",
		"markdown",
		"obsidian"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"commander": "^14.0.1",
		"marked": "^15.0.12"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"vitest": "^3.2.4"
	}
}
